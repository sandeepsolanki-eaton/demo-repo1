			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "ds_InputMovies",
								"type": "DatasetReference"
							},
							"name": "srcMovies"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "ds_Output2",
								"type": "DatasetReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "filterThrillerMovies"
						}
					],
					"scriptLines": [
						"parameters{",
						"     Movie_type as string",
						"}",
						"source(output(",
						"          MovieID as string,",
						"          Title as string,",
						"          Type as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     partitionBy('hash', 1)) ~> srcMovies",
						"srcMovies filter(equals(Type,$Movie_type),",
						"     partitionBy('hash', 1)) ~> filterThrillerMovies",
						"filterThrillerMovies sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     partitionFileNames:['Movies.csv'],",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     partitionBy('hash', 1)) ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/datasets/ds_InputMovies')]",
				"[concat(variables('workspaceId'), '/datasets/ds_Output2')]"
			]
